<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="0.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>kaufenapp</name>
    <description>
        An app that lets a store owner manage their store from their Android mobile device.
    </description>
    <author email="chrstianh_meier@hotmail.com">
        Christian Meier
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
